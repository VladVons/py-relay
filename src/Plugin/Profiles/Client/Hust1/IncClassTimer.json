{
    "Class":[
        {
            "Class":"TControlSleep",
            "Alias":"Sleep_1",
            "Parameter":{"Time":1}
        },
        {
            "Class":"TSensorTimeRangeDay",
            "Alias":"SensorTRD_Light",
            "Descr":"Day light",
            "Parameter":{
                "Ranges":[
                    { "On":"$<DayOn>", "Off": "$<DayOff>"}
                ], 
                "WaitValue":60},
            "Triggers":[
                {"Enable":true, "Always":"self.SetValue('$IRelay_2')"}
            ]
        },
        {
            "Class":"TSensorTimeRangeCycle",
            "Alias":"SensorTRC_Air",
            "Descr":"Air",
            "Parameter":{
                "Ranges":[
                    { "On":"$<MatochAirOn>", "Off": "$<MatochAirOff>"}
                ]
            },
            "Triggers":[
                {"Enable":false, "Always":"self.SetValue('$IRelay_3')"},
                {"Enable":true,  "Always":"self.SetValue('$IRelay_3', self.Value and (not self.InHours([['20:30', '21:30'], ['22:30', '23:30']])))"}
            ]
        },
        {
            "Class":"TSensorTimeRangeCycle",
            "Alias":"SensorTRC_Flash",
            "Descr":"Air",
            "Parameter":{
                "Ranges":[
                    { "On":"30S", "Off": "30S"}
                ]
            },
            "Triggers":[
                {"Enable":true, "Always":"self.SetValue('$IRelay_5')"}
            ]
        },
        {
            "Class":"TSensorTimeRangeDay",
            "Alias":"SensorTRD_Humid",
            "Descr":"Day Humid",
            "Parameter":{
                "Ranges":[
                    { "On":"01:00", "Off": "01:01"},
                    { "On":"03:30", "Off": "03:31"},
                    { "On":"20:00", "Off": "20:01"},
                    { "On":"22:30", "Off": "22:31"}
                ]
            },
            "Triggers":[
                {"Enable":true, "Always":"self.SetValue('$IRelay_1')"}
            ]
        },
        {
            "Class":"TSensorTimeRangeDayFadeWave",
            "Alias":"SensorTRDFW_Temper",
            "Descr":"Day temp wave",
            "Parameter":{
                "Ranges":[
                    { "On":"$<DayOn>", "Off": "$<DayOff>"}
                ],
                "Min": "$<MatochTempMin>", "Max": "$<MatochTempMax>"}
        }
    ]
}
