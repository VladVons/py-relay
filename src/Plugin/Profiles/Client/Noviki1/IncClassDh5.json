{
    "Class":[
       {
            "Class":"TProviderDeviceHivePinThread_DHT22",
            "Alias":"DH5_Provider",
            "Parameter":{"Host":"http://dh5.lan", "Pin":14}
        },
        {
            "Class":"TSensorProvider",
            "Alias":"DH5_Sensor_Temper",
            "Descr":"Inkubator_ST",
            "Parameter":{"ClassRef":"DH5_Provider", "Tag":"temperature", "WaitValue":600},
            "Triggers":[
                {"Enable":true, "If":"self.Value < 15", "Then":"self.SetValue('$Telegram')"}
            ]
        },
        {
            "Class":"TSensorProvider",
            "Alias":"DH5_Sensor_Humid",
            "Descr":"Inkubator_SH",
            "Parameter":{"ClassRef":"DH5_Provider", "Tag":"humidity", "WaitValue":600}
        }
    ]
}
