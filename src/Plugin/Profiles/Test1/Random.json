    {
        "Class":"TSensorRandom",
        "Alias":"Random_10_30",
        "Descr":"Random",
        "Parameter":{"Begin":10, "End":30},
        "Triggers":[
            {"Enable":true, "If":"self.Value > 15", "Then":"self.CountSkip = 10"},
            {"Enable":true, "If":"self.Value > 20", "Then":"self.Break('Test')"},
            {"Enable":true, "Always":"self.SetValue('$WriteConsoleStart', 1000)"},
            {"Enable":true, "Label":"Test", "If":"self.Value > 5", "Then":"if self.CountIf > 10: self.SetIf('$WriteLog')"}
        ],
        "Action":[
            {"OnValue": {"Enable":true,  "ClassRef":"WriteConsole2"}}
        ]
    },

    {
        "Class":"TSensorRandom",
        "Alias":"RandomEx_10_30",
        "Descr":"Random",
        "Parameter":{"Begin":10, "End":30},
        "Action":[
            {"OnValue": {"Enable":false,  "ClassRef":"WriteConsole1"}}
        ],
        "Start":[
            {"Enable":false,  "Always":"self.SetValue('$WriteConsoleStart',1000)"}
        ],
        "Checks":[
            {"Enable":false,  "If":"self.GetValue('$Random_1000_3000') > 2500"}
        ],
        "Triggers":[
            {"Enable":true, "If":"self.Value > 28", "Then":"self.SetIf('$WriteConsole1')"}
        ],
        "Finish":[
            {"Enable":false,  "Always":"self.SetValue('$WriteConsoleFinish',2000)"}
        ]
    }
]
