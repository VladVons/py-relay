{
    "Class":[
        {
            "Class":"TSensorRandom",
            "Alias":"Random_10_30",
            "Descr":"Random generator",
            "Parameter":{"Begin":10, "End":30},
            "Triggers":[
                {"Enable":false, "Always":"self.File('DH2.py')"},
                {"Enable":true, "If":"self.Value > 20", "Then":"self.Break('Test')"},
                {"Enable":true, "Always":"self.SetValue('$WriteConsoleRand', 1000)"},
                {"Enable":true, "Label":"Test", "If":"self.Value > 15", "Then":"if self.CountIf > 10: self.SetIf('$WriteLog')"}
            ]
        },
        {
            "Class":"TSensorRandomNone",
            "Alias":"RandomNone_10_100",
            "Descr":"Random generator with None value",
            "Parameter":{"Begin":10, "End":100},
            "Triggers":[
                {"Enable":false, "If":"self.Error",      "Always":"self.SetValue('$WriteConsole3', 111); self.Break()"},
                {"Enable":false, "If":"self.Value < 40", "Always":"self.SetIf('$WriteConsole2')"}
            ]
        },
        {
            "Class":"TSensorRandom",
            "Alias":"Random_15_25",
            "Descr":"Random generator static",
            "Parameter":{"Begin":15, "End":25, "Refresh":200},
            "Triggers":[
                {"Enable":false, "If":"self.Value < self.Class('$SensorTRDF_2').Value", "Always":"self.SetIf('$WriteConsole2')"},
                {"Enable":true,  "If":"self.Value < self.Thermostat(20, 1)", "Then":"self.SetIf('$WriteConsole2')"}
            ]
        },
        {
            "Class":"TSensorRandom",
            "Alias":"Random_100_104",
            "Parameter":{"Begin":100, "End":110}
        },
        {
            "Class":"TSensorCycleInc",
            "Alias":"SensorCycleInc",
            "Parameter":{"Begin":15, "End":25},
            "Triggers":[
                {"Enable":true, "If":"self.Thermostat(20, 1)", "Always":"self.SetIf('$WriteConsole2')"}
            ]
        },
        {
            "Class":"TSensorCycleDec",
            "Alias":"SensorCycleDec",
            "Parameter":{"Begin":15, "End":25},
            "Triggers":[
                {"Enable":true, "If":"self.Thermostat(20)", "Always":"self.SetIf('$WriteConsole2')"}
            ]
        },
        {
            "Class":"TSensorRandom",
            "Alias":"RandomEx_11_31",
            "Descr":"Random",
            "Parameter":{"Begin":11, "End":31},
            "Action":[
                {"OnValue": {"Enable":false,  "ClassRef":"WriteConsole1"}}
            ],
            "Start":[
                {"Enable":false,  "Always":"self.SetValue('$WriteConsoleStart',1000)"}
            ],
            "Checks":[
                {"Enable":false,  "If":"self.GetValue('$Random_1000_3000') > 2500"}
            ],
            "Triggers":[
                {"Enable":true, "If":"self.Value > 28", "Then":"self.SetIf('$WriteConsole1')"}
            ],
            "Finish":[
                {"Enable":false,  "Always":"self.SetValue('$WriteConsoleFinish',2000)"}
            ]
        }
    ]
}
