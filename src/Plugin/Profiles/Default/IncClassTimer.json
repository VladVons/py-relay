{
    "Class":[
        {
            "Class":"TSensorTimeRangeDay",
            "Alias":"SensorTimeRangeDay",
            "Descr":"Day light",
            "Parameter":{"Ranges":[{ "On":"02:00", "Off": "18:00"}]},
            "Triggers":[
                {"Enable":true, "Always":"self.SetValue('$IRelay_1')"}
            ]
        },
        {
            "Class":"TSensorTimeRangeCycle",
            "Alias":"SensorTimeRangeCycle",
            "Descr":"Air",
            "Parameter":{"Ranges":[{"On":"3S", "Off": "3S"}]},
            "Checks":[
                {"Enable":false, "If":"self.InHour('08:14', '08:15')"}
            ],
            "Triggers":[
                {"Enable":true, "Always":"self.SetValue('$WriteLog', self.Value and (not self.InHours([['07:08', '07:09'], ['19:55', '19:56']])))"}
            ]
        },
        {
            "Class":"TSensorTimeRangeDay",
            "Alias":"SensorTRD_Humid",
            "Descr":"Day Humid",
            "Parameter":{"Ranges":[
                { "On":"01:00", "Off": "01:01"}, 
                { "On":"12:10", "Off": "12:20"}, 
                { "On":"20:00", "Off": "20:01"}, 
                { "On":"22:30", "Off": "22:31"}
            ], "Refresh":2},
            "Triggers":[
                {"Enable":false, "Always":"self.SetValue('$IRelay_4')"}
            ]
        },
        {
            "Class":"TSensorTimeRangeDayFadeWave",
            "Alias":"SensorTRDF_1",
            "Descr":"Day temp wave  sum",
            "Parameter":{"Ranges":[{ "On":"4:10", "Off": "4:30"}, { "On":"4:40", "Off": "4:50"}], "Min": -10, "Max": 10}
        },
        {
            "Class":"TSensorTimeRangeDayFadeWave",
            "Alias":"SensorTRDF_2",
            "Descr":"Day temp wave",
            "Parameter":{"Ranges":[{ "On":"10:45", "Off": "11:00"}], "Min": 15, "Max": 25}
        }
    ]
}
