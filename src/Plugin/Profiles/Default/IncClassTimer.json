{
    "Class":[
        {
            "Class":"TSensorTimeRangeDay",
            "Alias":"SensorTimeRangeDay",
            "Descr":"Day light",
            "Parameter":{"Ranges":[{ "On":"02:00", "Off": "18:00"}]},
            "Triggers":[
                {"Enable":true, "Always":"xASetValue('$IRelay_1')"}
            ]
        },
        {
            "Class":"TSensorTimeRangeCycle",
            "Alias":"SensorTimeRangeCycle",
            "Descr":"Air",
            "Parameter":{
                "Time":[{"On":"3S", "Off": "3S"}, {"On":"5S", "Off": "5S"}],
                "Value":[{"On":1, "Off": 0}, {"On":11, "Off": 10}]
            },
            "Checks":[
                {"Enable":false, "If":"xInHour('08:14', '08:15')"}
            ],
            "Triggers":[
                {"Enable":false, "Always":"xASetValue('$WriteLog', xValue and (not xInHours([['07:08', '07:09'], ['19:55', '19:56']])))"}
            ]
        },
        {
            "Class":"TSensorTimeRangeDay",
            "Alias":"SensorTRD_Humid",
            "Descr":"Day Humid",
            "Parameter": {
                "Time":[
                    { "On":"01:00", "Off": "01:01"}, 
                    { "On":"13:50", "Off": "13:54"}, 
                    { "On":"20:00", "Off": "20:01"}, 
                    { "On":"22:30", "Off": "22:31"}
                ], 
                "Value":[
                    { "On": 11, "Off": 10}
                ],
                "Refresh":2
            },
            "Triggers":[
                {"Enable":false, "Always":"xASetValue('$IRelay_4')"}
            ]
        },
        {
            "Class":"TSensorTimeRangeDayFadeLine",
            "Alias":"SensorTRDFL_Humid",
            "Descr":"Month Humid",
            "Parameter": {
                "Time":[
                    { "On":"01:00", "Off": "02:00"}, 
                    { "On":"04:00", "Off": "05:00"},
                    { "On":"05:30", "Off": "05:55"}
                ], 
                "Value":[
                    { "On": 0, "Off": 300}
                ]
            }
        },
        {
            "Class":"TSensorTimeRangeDayFadeWave",
            "Alias":"SensorTRDF_1",
            "Descr":"Day temp wave  sum",
            "Parameter":{"Ranges":[
                { "On":"4:10", "Off": "4:30"}, 
                { "On":"4:40", "Off": "4:50"}
            ], 
            "Min": -10, "Max": 10}
        },
        {
            "Class":"TSensorTimeRangeDayFadeWave",
            "Alias":"SensorTRDF_2",
            "Descr":"Day temp wave",
            "Parameter":{"Ranges":[{ "On":"10:45", "Off": "11:00"}], "Min": 15, "Max": 25}
        },
        {
            "Class":"TSensorTimeRangeDayFadeWave",
            "Alias":"SensorTRDFW_Humid",
            "Descr":"Day humid wave",
            "Parameter": {
                "Time":[{"On": "$<cDayOn>", "Off": "$<cDayOff>"}],
                "Value":[{"On": "$<cMatoch2_HumidMin>", "Off": "$<cMatoch2_HumidMax>"}],
                "Invert": true
            }
        }
    ]
}
